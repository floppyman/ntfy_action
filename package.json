	},
	"homepage": "https://github.com/actions/javascript-action#readme",
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.0",
		"axios": "^1.7.9"
	},
	"devDependencies": {
		"@eslint/js": "^9.20.0",
		"@vercel/ncc": "^0.38.3",
		"babel-eslint": "^10.1.0",
		"eslint": "^9.20.0",
		"globals": "^15.14.0",
		"jest": "^29.7.0",
